<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lmzfm.dao.ApplyListMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.lmzfm.model.ApplyList">
                    <id column="id" property="id"/>
                    <result column="app_id" property="appId"/>
                    <result column="content" property="content"/>
                    <result column="status" property="status"/>
                    <result column="app_time" property="appTime"/>
                    <result column="app_author" property="appAuthor"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, app_id, content, status, app_time, app_author
        </sql>

</mapper>